@{ Layout = null; }

<section class="col-md-12">
    <table class="table table-striped table-hover" style="position: relative">
        <thead>
            <tr class="text-warning">
                <td><strong>UserName</strong></td>
                <td><strong>Email</strong></td>
                <td><strong>EmailConfirmed</strong></td>
                <td><strong>PhoneNumber</strong></td>
                <td><strong>PhoneNumberConfirmed</strong></td>
            </tr>
        </thead>
        <tbody data-bind="foreach: Users">
            <tr>
                <td data-bind="text: userName"></td>
                <td data-bind="text: email"></td>
                <td data-bind="text: emailConfirmed"></td>
                <td data-bind="text: phoneNumber"></td>
                <td data-bind="text: phoneNumberConfirmed"></td>
            </tr>
        </tbody>
    </table>
</section>
